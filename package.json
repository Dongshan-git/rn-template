{
  "name": "rn-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android:dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "android:staging": "cd ./android && cross-env ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:prod": "cd ./android && cross-env ENVFILE=.env.prod ./gradlew app:assembleRelease",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "prettier": "prettier --write --cache --cache-strategy metadata \"**/*.{tsx,ts}\"",
    "clean": "react-native-clean-project",
    "postinstall": "npx husky install && npx patch-package",
    "commit": "git-cz",
    "tsc": "tsc -p tsconfig.json",
    "pod": "cd ios && bundle exec pod install && cd ..",
    "publish:android": "cd ./android && fastlane publish",
    "publish:ios": "cd ./ios && fastlane publish"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.4",
    "@byron-react-native/refresh-control": "^1.2.5",
    "@ebay/nice-modal-react": "^1.2.8",
