{
  "name": "rn-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android:dev": "ENVFILE=.env.dev react-native run-android",
    "android:staging": "cd ./android && ENVFILE=.env.staging ./gradlew app:assembleRelease",
    "android:prod": "cd ./android && ENVFILE=.env.prod ./gradlew app:assembleRelease",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "eslint": "eslint --fix --ext .ts,.tsx src/",
    "prettier": "prettier --check src/**/*.tsx --write",
    "clean": "react-native-clean-project",
    "postinstall": "npx husky install",
    "commit": "git-cz",
    "tsc": "tsc -p tsconfig.json",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "test": "jest --passWithNoTests --detectOpenHandles --watch"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run eslint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.11",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "^2.2.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@shopify/restyle": "^1.4.0",
    "@td-design/react-native": "^3.6.1",
    "@td-design/react-native-echarts": "^1.3.0",
    "@td-design/react-native-image-picker": "^1.5.4",
    "@td-design/react-native-picker": "^1.6.7",
    "@td-design/rn-hooks": "^2.0.0",
    "immer": "^9.0.6",
    "jotai": "^1.4.3",
    "jotai-modular": "^1.3.1",
    "lodash": "^4.17.21",
    "rc-field-form": "^1.22.0",
    "react": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-native": "^0.66.3",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^3.2.6",
    "react-native-config": "^1.4.5",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.2.0",
    "react-native-reanimated": "^2.2.4",
    "react-native-redash": "^16.2.2",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-svg": "^12.1.1",
    "react-native-webp-format": "^1.1.2",
    "react-native-webview": "^11.14.3",
    "rn-fetch-blob": "^0.12.0",
    "swr": "^1.0.1",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@bam.tech/react-native-make": "^3.0.3",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@td-design/pont-engine": "1.0.3",
    "@td-design/svgicon-cli": "^1.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/react-native": "^0.66.4",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.3.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-replace-hooks": "^1.0.3",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react-native-clean-project": "^3.6.4",
    "react-test-renderer": "^17.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
